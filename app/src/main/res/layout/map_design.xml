                            android:layout_height="15dp"
                            android:layout_alignLeft="@+id/exchangeIcon"
                            android:layout_below="@+id/exchangeIcon"
                            android:layout_marginTop="5dp"
                            android:src="@drawable/quanity"/>
                        <!--android:src="@drawable/service_false" -->

                        <TextView
                            android:id="@+id/quitity"
                            style="@style/RobotoTextViewStyle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/returnIcon"
                            android:layout_marginLeft="10dp"
                            android:layout_toEndOf="@+id/returnIcon"
                            android:layout_toRightOf="@+id/returnIcon"
                            android:text="QUANTITY :"
                            android:textColor="@color/secondary_text"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/quitity_tex"
                            style="@style/RobotoTextViewStyle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/returnIcon"
                            android:layout_marginLeft="10dp"
                            android:layout_toEndOf="@+id/quitity"
                            android:layout_toRightOf="@+id/quitity"
