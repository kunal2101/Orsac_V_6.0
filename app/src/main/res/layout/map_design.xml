                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/exchangeIcon"
                            android:layout_marginLeft="10dp"
                            android:textStyle="bold"
                            android:layout_toRightOf="@+id/min_name"
