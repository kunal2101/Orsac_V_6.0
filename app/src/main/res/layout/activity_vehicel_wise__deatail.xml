<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <include layout="@layout/toolbar_orsac" />

            <customfonts.MyTextView
                android:id="@+id/etp_title"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_marginBottom="5dp"
                android:background="@color/white_smoke"
                android:gravity="center"
                android:text="ETP No - ------------ "
                android:textColor="@color/fb_blue"
                android:textSize="17sp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#F3F4F6"
                android:orientation="horizontal"
                android:padding="12dp">


                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/ic_about_us"
                    android:gravity="center" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="Vendor :"
                    android:textColor="@color/text"
                    android:textSize="16dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/circles"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginRight="7dp"
                    android:layout_marginTop="5dp"
                    android:gravity="right"
                    android:text="----------"

                    android:textColor="@color/textred"
                    android:textSize="15dp"
                    android:textStyle="bold" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/ic_bill" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="Last Polling Time:"
                    android:textColor="@color/text"
                    android:textSize="16dp"
                    android:textStyle="bold" />


                <TextView
                    android:id="@+id/valid_date"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp"
                    android:gravity="right"
                    android:textStyle="bold"
                    android:textSize="15dp"
                    android:text="------------------"
                    android:textColor="@color/colorPrimary" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/truck" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="Vehicle Reg No :"
                    android:textColor="@color/text"
                    android:textSize="16dp"
                    android:textStyle="bold" />


                <TextView
                    android:id="@+id/vehi_num"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp"
                    android:gravity="right"
                    android:textStyle="bold"
                    android:textSize="15dp"
                    android:text="------------"
                    android:textColor="@color/accent_color"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/ic_logistics_delivery_truck_and_clock" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="VTU IMEI No :"
                    android:textColor="@color/text"
                    android:textSize="16dp"
                    android:textStyle="bold"/>


                <TextView
                    android:id="@+id/code_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp"
                    android:gravity="right"
                    android:textStyle="bold"
                    android:textSize="15dp"
                    android:text="------------"
                    android:textColor="@color/fb_blue" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/ic_air_conditioner" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="Current Date :"
                    android:textColor="@color/text"
                    android:textSize="16dp"
                    android:textStyle="bold"/>


                <TextView
                    android:id="@+id/current_date_time"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp"
                    android:gravity="right"
                    android:textStyle="bold"
                    android:textSize="15dp"
                    android:text="------------"
                    android:textColor="@color/fb_blue" />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/stop" />

                <TextView
                    android:id="@+id/txt_near_by_me"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="Vehicle Near By Me :"
                    android:textColor="@color/text"
                    android:textSize="16dp"
                    android:textStyle="bold"/>


                <TextView
                    android:id="@+id/vehicle_near_by_truck"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp"
                    android:gravity="right"
                    android:textStyle="bold"
                    android:textSize="15dp"
                    android:text="------------"
                    android:textColor="@color/fb_blue" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/ic_about_us" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="VTS Id/SL No :"
                    android:textColor="@color/text"
                    android:textSize="16dp"
                    android:textStyle="bold"/>


                <TextView
                    android:id="@+id/id_sl_no"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp"
                    android:gravity="right"
                    android:textStyle="bold"
                    android:textSize="15dp"
                    android:text="------------"
                    android:textColor="@color/fb_blue" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:visibility="gone"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/location" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="GPS :"
                    android:textColor="@color/text"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    />


                <TextView
                    android:id="@+id/gps"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp"
                    android:gravity="right"
                    android:textStyle="bold"
                    android:textSize="15dp"
                    android:text="------------"
                    android:textColor="@color/red_light"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/ic_circle" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="Sim Validity Date :"
                    android:textColor="@color/text"
                    android:textSize="16dp"
                    android:textStyle="bold"/>


                <TextView
                    android:id="@+id/start_day"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp"
                    android:gravity="right"
                    android:textStyle="bold"
                    android:textSize="15dp"
                    android:text="------------"
                    android:textColor="@color/dot_dark_screen3" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:visibility="visible"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/ic_ingination" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="Sim Validity Status:"
                    android:textColor="@color/text"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    />


                <TextView
                    android:id="@+id/ign"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp"
                    android:gravity="right"
                    android:textStyle="bold"
                    android:textSize="15dp"
                    android:text="------------"
                    android:textColor="@color/green"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:visibility="visible"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/ic_live_news_report" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="VTU Status:"
                    android:textColor="@color/text"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    />


                <TextView
                    android:id="@+id/vtu_status"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="end"
                    android:layout_marginRight="20dp"
                    android:background="@drawable/border"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="-------"
                    android:layout_marginLeft="25dp"
                    android:textColor="@color/green"
                    android:textSize="15dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/recharge_due"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="Recharge Due"
                    android:background="@drawable/button_selector"
                    android:visibility="gone"
                    android:textColor="@color/white"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    />



            </LinearLayout>

            <androidx.cardview.widget.CardView
                android:id="@+id/address_card"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"

                android:clickable="true"
                android:visibility="gone"
                card_view:cardElevation="2dp"
                card_view:cardPreventCornerOverlap="false">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/addLabel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="8dp"
                        android:text="ADDRESS DETAIL"
                        android:textColor="@color/textred"
                        android:textStyle="bold"
                        android:layout_marginBottom="5dp"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/sou_title"
                        android:layout_below="@+id/addLabel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:padding="5dp"
                        android:text="Source :"
                        android:textColor="@color/text"
                        android:textSize="18dp"
                        android:textStyle="bold" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_below="@+id/addLabel"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/sou_title"
                        android:singleLine="true"
                        android:padding="5dp"
                        android:layout_marginTop="5dp"
                        android:textSize="15dp"
                        android:text="Utkal Metalics Ltd.,Utkal Metalics Ltd.(Kalunga)"/>
                    <View
                        android:id="@+id/view_one"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/sou_title"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/gray"/>
                    <TextView
                        android:id="@+id/des_tit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Destination :"
                        android:textSize="18dp"
                        android:textColor="@color/text"
                        android:padding="5dp"
                        android:layout_marginLeft="25dp"
                        android:layout_below="@+id/view_one"
                        android:textStyle="bold"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/des_tit"
                        android:text="K A I INTERNATIONAL PRIVATE LIMITED"
                        android:layout_below="@+id/view_one"
                        android:padding="5dp"
                        android:layout_marginTop="5dp"
                        android:singleLine="true"
                        android:layout_marginBottom="5dp"/>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/time_card"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:clickable="true"
                android:visibility="visible"
                card_view:cardElevation="2dp"
                card_view:cardPreventCornerOverlap="false">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/addLabel_se"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="8dp"
                        android:text="Mineral Trip Details"
                        android:textColor="@color/textred"
                        android:textStyle="bold"
                        android:layout_marginBottom="5dp"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/source_ti"
                        android:layout_below="@+id/addLabel_se"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:padding="5dp"
                        android:text="Source :"

                        android:textColor="@color/text"
                        android:textSize="18dp"
                        android:textStyle="bold" />
                    <customfonts.MyTextView
                        android:id="@+id/source"
                        android:layout_width="wrap_content"
                        android:layout_below="@+id/addLabel_se"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/source_ti"
                        android:singleLine="true"
                        android:padding="5dp"
                        android:layout_marginTop="5dp"
                        android:textSize="15dp"
                        android:text="--------------"/>
                    <View
                        android:id="@+id/view_one_se"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/source"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/gray"/>
                    <TextView
                        android:id="@+id/des_tit_se"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Destination :"
                        android:textSize="18dp"
                        android:textColor="@color/text"
                        android:padding="5dp"
                        android:layout_marginLeft="25dp"
                        android:layout_below="@+id/view_one_se"
                        android:textStyle="bold"/>
                    <customfonts.MyTextView
                        android:id="@+id/destination"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/des_tit_se"
                        android:text="---------------------"
                        android:layout_below="@+id/view_one_se"
                        android:padding="5dp"
                        android:layout_marginTop="5dp"
                        android:singleLine="true"
                        android:textSize="15sp"
                        android:layout_marginBottom="5dp"/>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/policy_card"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:clickable="true"
                android:visibility="visible"
                card_view:cardElevation="2dp"
                card_view:cardPreventCornerOverlap="false">

                <RelativeLayout
                    android:id="@+id/policyLay"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="false">

                    <TextView
                        android:id="@+id/policyLabel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="8dp"
                        android:text="TRIP DETAIL"
                        android:textColor="@color/fb_blue"
                        android:textStyle="bold"
                        android:layout_marginBottom="5dp"
                        android:textSize="15sp" />

                    <ImageView
                        android:id="@+id/exchangeIcon"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_alignLeft="@+id/policyLabel"
                        android:layout_below="@+id/policyLabel"
                        android:layout_marginTop="10dp"
                        android:src="@drawable/miner"/>
                    <!--android:src="@drawable/service_true" -->

                    <TextView
                        android:id="@+id/min_name"
                        style="@style/RobotoTextViewStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/exchangeIcon"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/exchangeIcon"
                        android:text="@string/mine"
                        android:textColor="@color/secondary_text"
                        android:textSize="12sp" />

                    <ImageView
                        android:id="@+id/returnIcon"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_alignLeft="@+id/exchangeIcon"
                        android:layout_below="@+id/exchangeIcon"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/quanity"/>
                    <!--android:src="@drawable/service_false" -->

                    <TextView
                        android:id="@+id/quitity"
                        style="@style/RobotoTextViewStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/returnIcon"
                        android:layout_marginLeft="10dp"
                        android:layout_toEndOf="@+id/returnIcon"
                        android:layout_toRightOf="@+id/returnIcon"
                        android:text="@string/quantity"
                        android:textColor="@color/secondary_text"
                        android:textSize="12sp" />


                    <ImageView
                        android:id="@+id/creditIcon"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_alignLeft="@+id/exchangeIcon"
                        android:layout_below="@+id/returnIcon"
                        android:src="@drawable/speedometer"
                        android:layout_marginTop="5dp" />
                    <!-- -->

                    <TextView
                        android:id="@+id/credit"
                        style="@style/RobotoTextViewStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/creditIcon"
                        android:layout_alignLeft="@+id/quitity"
                        android:layout_alignStart="@+id/quitity"
                        android:text="@string/speed"
                        android:visibility="visible"
                        android:textColor="@color/secondary_text"
                        android:textSize="12sp" />

                    <ImageView
                        android:id="@+id/dirIcon"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_alignLeft="@+id/exchangeIcon"
                        android:layout_below="@+id/creditIcon"
                        android:layout_marginBottom="10dp"
                        android:src="@drawable/compass"
                        android:layout_marginTop="5dp" />
                    <!-- -->

                    <TextView
                        android:id="@+id/dir_text"
                        style="@style/RobotoTextViewStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/dirIcon"
                        android:layout_alignLeft="@+id/quitity"
                        android:layout_alignStart="@+id/quitity"
                        android:text="@string/direc"
                        android:visibility="visible"
                        android:textColor="@color/secondary_text"
                        android:textSize="12sp" />

                    <View
                        android:id="@+id/policyDivider"
                        android:layout_width="wrap_content"
                        android:layout_height="1dp"
                        android:layout_below="@+id/dir_text"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="10dp"
                        android:background="@color/separator_color"></View>

                    <TextView
                        android:id="@+id/policyText"
                        style="@style/RobotoTextViewStyle"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/exchangeIcon"
                        android:layout_below="@+id/policyDivider"
                        android:layout_marginBottom="10dp"
                        android:paddingTop="5dp"
                        android:textColor="@color/secondary_text"
                        android:textSize="12sp" />

                </RelativeLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/gps_card"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="15dp"
                android:clickable="true"

                android:visibility="visible"
                card_view:cardElevation="2dp"
                card_view:cardPreventCornerOverlap="false">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/addLabel_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="8dp"
                        android:text="OTHER DETAIL"
                        android:textColor="@color/primary"
                        android:textStyle="bold"
                        android:layout_marginBottom="5dp"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/sou_title_time"
                        android:layout_below="@+id/addLabel_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:padding="5dp"
                        android:text="Vehicle Type :"
                        android:textColor="@color/text"
                        android:textSize="18dp"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/pick_date"
                        android:layout_width="wrap_content"
                        android:layout_below="@+id/addLabel_time"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/sou_title_time"
                        android:singleLine="true"
                        android:padding="5dp"
                        android:layout_marginTop="5dp"
                        android:textSize="15dp"
                        android:text="2017-01-01"/>
                    <View
                        android:id="@+id/view_one_time"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/sou_title_time"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/gray"/>
                    <TextView
                        android:id="@+id/des_tit_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Data Refresh Time:"
                        android:textSize="18dp"
                        android:textColor="@color/text"
                        android:padding="5dp"
                        android:layout_marginLeft="25dp"
                        android:layout_below="@+id/view_one_time"
                        android:textStyle="bold"/>
                    <TextView
                        android:id="@+id/pick_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/des_tit_time"
                        android:text="07:22:01"
                        android:layout_below="@+id/view_one_time"
                        android:padding="5dp"
                        android:textSize="15sp"
                        android:layout_marginTop="5dp"
                        android:singleLine="true"
                        android:layout_marginBottom="5dp"/>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:visibility="gone">

                <customfonts.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Please call"
                    android:textColor="@color/text"
                    android:textSize="13dp" />

                <customfonts.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" 011 2382 925"
                    android:textColor="#59c861"
                    android:textSize="13dp" />

                <customfonts.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" to get your new PIN"
                    android:textColor="@color/text"
                    android:textSize="13dp" />

            </LinearLayout>

        </LinearLayout>

    </ScrollView>
    <TextView
        android:id="@+id/rose_footer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
